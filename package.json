{
  "name": "general-store",
  "version": "1.0.0",
  "description": "Simple, flexible store implementation for Flux.",
  "main": "dist/GeneralStore.js",
  "scripts": {
    "build:commonjs": "BABEL_ENV=commonjs babel src --ignore *-test.js --out-dir dist",
    "build:umd": "NODE_ENV=development webpack --config ./webpack.config.development.js src/GeneralStore.js build/general-store.js",
    "build:umd:min": "NODE_ENV=production webpack --config ./webpack.config.production.js -p src/GeneralStore.js build/general-store.min.js",
    "build": "npm run build:commonjs && npm run build:umd && npm run build:umd:min",
    "build-and-test": "npm run build && npm test",
    "check": "flow check",
    "clean": "rm -rf ./build && rm -rf dist",
    "lint": "eslint ./src",
    "prepublish": "npm run clean && npm test && npm run build",
    "test": "npm run check && npm run lint && jest"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/HubSpot/general-store"
  },
  "keywords": [
    "flux",
    "store",
    "react"
  ],
  "files": [
    "dist",
    "build"
  ],
  "author": "Colby Rabideau",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/HubSpot/general-store/issues"
  },
  "homepage": "https://github.com/HubSpot/general-store",
  "peerDependencies": {
    "react": ">= 0.14"
  },
  "devDependencies": {
    "babel": "^6.5.2",
    "babel-cli": "^6.10.1",
    "babel-core": "^6.10.4",
    "babel-eslint": "^6.1.0",
    "babel-jest": "^13.0.0",
    "babel-loader": "^6.2.4",
    "babel-plugin-transform-class-properties": "^6.10.2",
    "babel-polyfill": "^6.9.1",
    "babel-preset-es2015": "^6.9.0",
    "babel-preset-react": "^6.11.1",
    "babel-preset-stage-2": "^6.11.0",
    "enzyme": "^2.3.0",
    "eslint": "^3.0.0",
    "eslint-config-hubspot": "^6.2.0",
    "eslint-plugin-react": "^5.2.2",
    "flow-bin": "^0.27.0",
    "flux": "^2.0.1",
    "immutable-is": "^3.7.4",
    "invariant": "^2.2.1",
    "jest-cli": "^12.1.1",
    "react": "^15.2.0",
    "react-addons-test-utils": "^15.2.0",
    "react-dom": "^15.2.0",
    "webpack": "^1.13.1"
  },
  "jest": {
    "testPathDirs": [
      "<rootDir>/src"
    ],
    "unmockedModulePathPatterns": [
      "immutable-is",
      "invariant",
      "<rootDir>/src/dispatcher/DispatcherInterface.js",
      "<rootDir>/src/uniqueid",
      "<rootDir>/src/utils/ObjectUtils.js"
    ]
  }
}
